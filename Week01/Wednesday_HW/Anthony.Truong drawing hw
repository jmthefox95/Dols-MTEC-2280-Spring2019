Void Setup() 
{
Size(800, 800);
Background (0);
}
Void draw () // looping
{
 // ellipse (mouseX, mouseY, 50, 50);
Fill (255);
ellipse(mouseX, mouseY, 50, 50);
If (mousePressed && (mouseButton == LEFT))
Fill (random ( 0, 255), random (0, 225), random (0, 225));
  ellispse(mouseX, mouseY, 50 , 50);

}




