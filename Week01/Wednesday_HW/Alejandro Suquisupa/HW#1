//Alejandro Suquisupa

int Circle=0;
float growth=.2;

void setup() {
  size (600,600); 
}

void draw() {
  background(0);
  
fill(255);
ellipse(300,300, 0 + growth, 0 + growth);

fill(130, 24, 7);
ellipse(300,300, 0 + growth/2, 0 + growth/2);

fill(32,204,216);
ellipse(300,300, 0 + growth/3, 0 + growth/3);

growth= growth + .2;
}
