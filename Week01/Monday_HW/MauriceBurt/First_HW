//Set parameters of canvas size with width and height in pixel units  

size(800,650);

//background color rgb values 0-255
// black background

background(0,0,0);
////////////////////////////////////////////////////////////////////////////////

// the cross hairs /////////////////////////////////////////////////////////////
//line(x1, y1, x2, y2);
strokeWeight(5);

stroke (255, 255, 255);
//horizontal line
line (155, 300, 650, 300);

////cross hairs on top//////////////////////////////////////////////////////////////

line(380, 85, 418, 85);

line(380, 126, 418, 126);

line(380, 174, 418, 174);

line(380, 225, 418, 225);

//////cross hairs on bottom///////////////////////////////////////////////////////

line(380, 375, 418, 375);

line(380, 420, 418, 420);

line(380, 475, 418, 475);

line(380, 515, 418, 515);

///////////////////////////////////////////////////////////////////////////////////

/////crosshairs on left side//////////

line(178, 285, 178, 315);

line(225, 285, 225, 315);

line(275, 285, 275, 315);

line(325, 285, 325, 315);

///////////////////////////////////////////////////////////////////////////

//////crosshairs on right side////////////////////////////////////////

line(473, 285, 473, 315);

line(520, 285, 520, 315);

line(571, 285, 571, 315);

line(622, 285, 622, 315);

/////////////////////////////////////////////////////////////////////////

// verticle line///////////////////////////////////////////////////////
line (398, 65, 398, 535);
/////////////////////////////////////////////////////////////////////////////////
////ellipse


///////red dot//////////////////////////////////////////////////////

fill(252, 0 , 0, 200); // color rgb

ellipse(400, 300, 100, 100);

////////////////////////////////////////////////////////////////////

///////scope///////////////////////////////////////////
fill(0, 101 , 252, 13);

ellipse(400, 300, 200, 200);

ellipse(400, 300, 300, 300);

ellipse(400, 300, 400, 400);

////////////////////////////////////////////////////////////////////////

// eyeball ellipse ////////////////////////////////////////

//fill(255, 255, 255, 25);

ellipse(400, 300, 500, 470);

ellipse(400, 300, 600, 470);

/////////////blue background//////////////////////////////////

ellipse(400, 300, 600, 470);
///////////////////////////////////////////
